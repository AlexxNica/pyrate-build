CXX = g++
CXX_FLAGS = -Wall -pedantic
rule compile_cpp
  command = $CXX $CXX_FLAGS ${opts} -MMD -MT $out -MF $out.d -c $in -o $out
  description = compile(cpp) $out
  depfile = $out.d
  deps = gcc

LINKER_EXE = gcc
LINKER_EXE_FLAGS = 
rule link_exe_786d8208a03cca3310ca96b2ebf2ff59
  command = $LINKER_EXE $LINKER_EXE_FLAGS -lstdc++ -o $out $in
  description = link(exe) $out

build test_1cfedb0e86f69ad43c2ba451c73bb586.o: compile_cpp test.cpp
build foo_a30d37c95b97852fd6dc8f4a1cf0a8e1.o: compile_cpp foo.cpp
build example03_debug.bin: link_exe_786d8208a03cca3310ca96b2ebf2ff59 test_1cfedb0e86f69ad43c2ba451c73bb586.o foo_a30d37c95b97852fd6dc8f4a1cf0a8e1.o
build test_ea06e0f15a7fb50d00928f0d8923fdef.o: compile_cpp test.cpp
  opts = -O3
build foo_b323f8bc970eb8ff180102d50bd99af1.o: compile_cpp foo.cpp
  opts = -O3
build example03_release.bin: link_exe_786d8208a03cca3310ca96b2ebf2ff59 test_ea06e0f15a7fb50d00928f0d8923fdef.o foo_b323f8bc970eb8ff180102d50bd99af1.o
build all: phony example03_debug.bin example03_release.bin
